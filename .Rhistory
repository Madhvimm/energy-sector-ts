i <- 1
for (p in 1:2) {
for (q in 1:4) {
if(p-1+d+q-1<=8) {
model <- Arima(oil_log_ts, order = c(p, d, q), include.drift = TRUE)
ls[i,] <- c(p, d, q, model$aic, model$bic, model$aicc)
i <- i + 1
}
}
}
